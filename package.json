  "devDependencies": {
    "@nuxt/module-builder": "^0.5.2",
    "@nuxt/schema": "^3.7.4",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "eslint": "^8.50.0",
    "nuxt": "^3.7.4",
    "typescript": "^5.2.2"
